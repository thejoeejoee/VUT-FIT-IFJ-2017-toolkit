' Multiplier listing

function multiply(a as integer, b as integer) as integer
    return a * b
end function

scope
    dim a as integer = 1
    
    do while a < 10 
    
        dim b as integer = 1
        
         do while b < 10 
            
            print multiply(a, b);
            
            b = b + 1
        loop
        print !"\n";
        a = a + 1
    loop
    
end scope